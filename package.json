{
  "name": "rac-language-polish",
  "version": "1.0.4",
  "description": "Polish messages for react-admin, the frontend framework for building admin applications on top of REST/GraphQL services",
  "keywords": [
    "react",
    "react-admin",
    "admin-on-rest",
    "translation"
  ],
  "homepage": "https://github.com/anthonykijoy/ra-language-polish#readme",
  "bugs": {
    "url": "https://github.com/anthonykijoy/ra-language-polish/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anthonykijoy/ra-language-polish.git"
  },
  "files": [
    "*.md",
    "dist",
    "src"
  ],
  "license": "MIT",
  "author": "Antoni Kijjaroentorn",
  "types": "dist/cjs/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build": "yarn run build-cjs && yarn run build-esm",
    "build-cjs": "rimraf ./lib && tsc",
    "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
    "test": "yarn build"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "ra-core": "^5.6.2"
  }
}
